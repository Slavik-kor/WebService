<?xml version="1.0" encoding="UTF-8"?>
<webservices xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/javaee_web_services_1_3.xsd" version="1.3">
	<webservice-description>
		<webservice-description-name>ServiceWSService</webservice-description-name>
		
		<port-component>
			<port-component-name>ServiceWS</port-component-name>
			<wsdl-service xmlns:pfx="http://ws.service.epam.edu/">
			pfx:ServiceWSService</wsdl-service>
			<wsdl-port xmlns:pfx="http://ws.service.epam.edu/">
			pfx:ServiceWSPort</wsdl-port>
			<enable-mtom>false</enable-mtom>
			<respect-binding>
				<enabled>true</enabled>
			</respect-binding>
			<service-impl-bean>
				<servlet-link>
				edu.epam.service.ws.ServiceWS</servlet-link>
			</service-impl-bean>
		</port-component>
	</webservice-description>
</webservices>
